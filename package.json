{
  "name": "rename-cli",
  "version": "7.0.3",
  "description": "A command line utility for renaming files",
  "main": "bin.js",
  "preferGlobal": true,
  "bin": {
    "rename": "./bin.js",
    "rname": "./bin.js"
  },
  "scripts": {
    "test": "jest",
    "build": "pkg --out-path bin -t latest-macos-x64,latest-linux-x64 .",
    "build-win": "pkg --out-path bin -t latest-win-x64 .",
    "chocolatey": "node chocolatey/packager.js"
  },
  "author": "Jordan Hotmann",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jhotmann/node-rename-cli.git"
  },
  "dependencies": {
    "async": "^3.2.0",
    "chalk": "^4.1.0",
    "cli-clear": "^1.0.4",
    "clipboardy": "^2.2.0",
    "date-fns": "^2.16.1",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.0",
    "inquirer": "^7.1.0",
    "jpeg-exif": "^1.1.4",
    "mp3tag.js": "^2.2.0",
    "fraction.js": "^4.0.0",
    "nunjucks": "^3.2.1",
    "opn": "^6.0.0",
    "path-exists": "^4.0.0",
    "readline-sync": "^1.4.10",
    "sequelize": "^6.3.5",
    "sqlite3": "^5.0.0",
    "terminal-kit": "^2.2.3",
    "traverse": "^0.6.6",
    "yargs": "^15.3.0"
  },
  "pkg": {
    "assets": [
      "lib/userData.js",
      "lib/userFilters.js",
      "node_modules/terminal-kit/**/*",
      "node_modules/opn/xdg-open"
    ]
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3"
  }
}
